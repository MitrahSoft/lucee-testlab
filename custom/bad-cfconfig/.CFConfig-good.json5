            "appenderBundleVersion": "5.5.2.15",
            "layoutClass": "lucee.commons.io.log.log4j2.layout.ClassicLayout",
            "layoutArguments": ""
        },
        "scope": {
            "appender": "resource",
            "appenderArguments": "path:{lucee-config}/logs/scope.log",
            "level": "error",
            "layout": "classic"
        },
        "scheduler": {
            "appender": "resource",
            "appenderArguments": "path:{lucee-config}/logs/scheduler.log",
            "level": "error",
            "layout": "classic"
        },
        "search": {
            "appender": "resource",
